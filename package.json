{
  "name": "funcky",
  "version": "0.4.1",
  "description": "Functional programming with express",
  "main": "build/module/index.js",
  "types": "build/module/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "node build.js",
    "deploy": "npm run build && npm publish --access public"
  },
  "bin": {
    "funcky": "build/runner.js",
    "funck": "node --experimental-loader=extensionless"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seo-rii/funcky.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seo-rii/funcky/issues"
  },
  "homepage": "https://github.com/seo-rii/funcky#readme",
  "dependencies": {
    "args-parser": "^1.3.0",
    "bson": "^5.3.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.9",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "short-uuid": "^4.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-ws": "^3.0.1",
    "@types/node": "^20.3.1",
    "clipboardy": "^3.0.0",
    "extensionless": "^1.4.5",
    "typescript": "^5.1.3"
  },
  "extensionless": {
    "lookFor": [
      "js",
      "mjs",
      "cjs",
      "json"
    ]
